mixin filaUsuario(usuario)
  tr
    td= usuario.email
    td= usuario.username
    td= usuario.edad
    td= usuario.municipio
    td= usuario.provincia
    td= usuario.codigo_postal
    td= usuario.curso

mixin filaPartidas(partida)
  tr
    td= partida.username
    td= partida.nombre
    td= partida.puntuacion
    td= partida.fecha



